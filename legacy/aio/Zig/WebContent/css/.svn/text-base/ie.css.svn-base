#manageAutopayTermsAndConditions+span,
#servicesPaymentTermsAndConditions+span {
	background-position: center left;
}
#loginRememberMe:checked+span,
#eulaChekBox:checked+span,
#payMyBillAutoPayCheckBox:checked+span,
#termsAndConditions:checked+span,
#manageAutopayTermsAndConditions:checked+span,
#servicesPaymentTermsAndConditions:checked+span {
	background: url(../img/checkbox_checked.png) no-repeat center;
	background-size: 17pt 17pt;
	background-position: top left;
}
