#loginRememberMe+span, 
#payMyBillAutoPayCheckBox+span, 
#termsAndConditions+span,
#manageAutopayTermsAndConditions+span,
#eulaChekBox+span,
#servicesPaymentTermsAndConditions+span {
	display: table-cell;
	float: none;
	background: url(../img/checkbox_unchecked.png) no-repeat center;
	background-size: 17pt 17pt;
	background-position: top left;
	width: 32px;
	height: 27px;
	vertical-align: top;
	cursor: pointer;
}
