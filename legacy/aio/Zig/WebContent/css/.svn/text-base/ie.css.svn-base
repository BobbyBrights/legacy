/*body:AFTER {
	content: 'default';
}*/
@-ms-viewport {
	width: device-width;
	user-zoom: fixed;
}
body { 
	/*background: url('../img/windows_phone/header_background.png') top no-repeat, url('../img/paper_background.png') bottom repeat-y;*/
	background-image: url('../img/windows_phone/header_background.png'), url('../img/paper_background.png');
	background-position: center top, center bottom;
	background-repeat: no-repeat, repeat-y;
/*font-family: "ITC Lubalin Graph Std" sans-serif;*/
	background-size: 100%;
	padding-left: 0%;
	padding-right: 0%;
	-ms-user-select: none;
	user-select: none;
	 -ms-touch-action: pan-y;
	/*  overflow: hidden; */
}

#blockUI {
	 -ms-touch-action: none;
}

#myPlanPage .spacer{
	height: 135px;
}
.list-container {
	margin: 0;
}
.select-label {
	display: none!important;
}
input {
	width: 100%;
	padding: 0;
	border: 2px solid;
	border-bottom-color: #CECECD;
	border-right-color: #AFAFAF;
	border-top-color: #9C9C9C;
	border-left-color: #AAA;
}
input:-ms-input-placeholder,
textarea:-ms-input-placeholder,
.placeholder {
	color:#999;
}
input[disabled],
input[readonly],
input:disabled,
textarea[disabled],
textarea[readonly],
textarea:disabled {
	background-color:rgb(235, 235, 228);
}
.header {
	position: static;
	width: 100%;
	box-sizing: border-box;
	margin: 0;
}
.header .top_left {
	text-align: left;
	position: absolute;
}
.header .top_right {
	text-align: right;
	position: absolute;
}

.page.failed_to_load .innerPage {
	visibility: hidden;
}
.page.failed_to_load:AFTER {
	content : "Failed to load page, try later";
	width: 100%;
	text-align: left;
	top: 187px;
	position: absolute;
}

/* *** FOOTER *** */
#footer {
	-ms-touch-action: none;
	position: absolute;
	height: 110px;
	top: 68px;
	width: 100%;
	background-image: url("../img/windows_phone/top_bar_bg.jpg");
	background-position: left;
	background-position: top;
	background-repeat: no-repeat;
	background-size: 100%;
	overflow: hidden;
	color: white;
}

.page {
	display: none;
	width: 100%;
	top: 0;
	left: 0;
	box-sizing: border-box;
	margin-top: 25px;
	padding-left: 3%;
	padding-right: 3%;
}

.page.no-footer {
	top: 47px;
}
/* ***** FORMS ***** */
form {
	margin: 0;
}

.button-bar {
	margin: 0;
}

form input {
	padding: 0 0 0 5px;
	-ms-box-sizing: border-box;
	box-sizing: border-box;
}

form .list-container .list-item {
	height: auto;
	border-bottom: 0px none;
	padding: 0;
}

.select-container {
	border-bottom: 2px solid #C7C7C7;
	margin: 0 auto;
	width: 60%;
	background: url('../img/down-button.png') no-repeat bottom right;
	padding-right: 7px;
}

.select-container .ui-icon {
	display: none;
}

.select-container select {
	border-width: 0px;
	background: transparent;
	width: 100%;
	padding: 0 5px;
}

form .list-container .select-container {
	margin: 0;
	width: 90%;
}

.list-item input[type="checkbox"]+label {
	color: transparent;
	border: 0px;
	padding: 0 5px 0 0;
	cursor: pointer;
}

/* EULA */

.eulaFixedBottom.dark-background {
	margin-left: 0px;
	margin-right: 0px;
}
